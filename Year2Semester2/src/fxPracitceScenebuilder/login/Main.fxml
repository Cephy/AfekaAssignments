            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="txtUserName" alignment="CENTER" layoutX="38.0" layoutY="77.0" promptText="UserName">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="32.0" layoutY="22.0" prefHeight="39.0" prefWidth="236.0" text="Status" textFill="#e11111">
